<% provide(:title, 'All users') %>
<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<%= will_paginate %>
			<table class="table">
				<thead>
					<tr>
						<th>User ID</th>
						<th>First Name</th>
						<th>Last Name</th>
						<th>Email</th>
						<th></th>
					</tr>
				</thead>
				<tbody>
					<%= render @users %>
				</tbody>
			</table>
			<%= will_paginate %>
		</div>
	</div>
</div>